{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/operators/professor_profile_retrieve.output.v1.json",
  "title": "Operator: Professor.Profile.Retrieve (output) (v1)",
  "allOf": [
    { "$ref": "operator_result_base.v1.json" },
    {
      "type": "object",
      "properties": {
        "result": {
          "type": ["object", "null"],
          "additionalProperties": false,
          "required": ["profile"],
          "properties": {
            "profile": {
              "type": "object",
              "additionalProperties": false,
              "required": ["professor_id", "profile_hash", "retrieved_at"],
              "properties": {
                "professor_id": { "$ref": "../common/defs.v1.json#/$defs/ProfessorId" },
                "profile_hash": { "$ref": "../common/defs.v1.json#/$defs/Hash" },
                "retrieved_at": { "$ref": "../common/defs.v1.json#/$defs/ISODateTime" },
                "canonical_url": { "$ref": "../common/defs.v1.json#/$defs/Url" },
                "sources": {
                  "type": "array",
                  "items": { "type": "string", "enum": ["platform", "canspider", "web", "manual"] },
                  "default": []
                },
                "raw_text_artifact": { "$ref": "../common/defs.v1.json#/$defs/ArtifactRef" },
                "extracted": { "type": "object", "default": {} }
              }
            }
          }
        }
      }
    }
  ]
}

