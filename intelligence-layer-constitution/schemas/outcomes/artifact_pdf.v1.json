{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schemas/outcomes/artifact_pdf.v1.json",
  "title": "Outcome: Artifact.PDF (v1)",
  "allOf": [
    { "$ref": "outcome_base.v1.json" },
    {
      "type": "object",
      "properties": {
        "outcome_type": { "const": "Artifact.PDF" },
        "payload": {
          "type": "object",
          "additionalProperties": false,
          "required": ["artifact"],
          "properties": {
            "artifact": { "$ref": "../common/defs.v1.json#/$defs/ArtifactRef" },
            "filename": { "type": "string" }
          }
        }
      }
    }
  ]
}

